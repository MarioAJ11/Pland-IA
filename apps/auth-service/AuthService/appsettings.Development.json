{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "USAR_VARIABLES_DE_ENTORNO_DB_*"
  },
  "JwtSettings": {
    "Secret": "USAR_VARIABLE_DE_ENTORNO_JWT_SECRET",
    "Issuer": "PlandIA.AuthService.Development",
    "Audience": "PlandIA.Client.Development",
    "AccessTokenExpirationMinutes": "30",
    "RefreshTokenExpirationDays": "7",
    "RefreshTokenExpirationDaysRememberMe": "30"
  },
  "AllowedHosts": "*",
  "DetailedErrors": true,
  "Environment": "Development"
}

